  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "echo \"todo: fix up typescript and implement linting\" # d2-style check",
    "format": "d2-style apply",
    "build:chap-lib": "yarn workspace @dhis2-chap/chap-lib build",
    "build:chap-eval": "yarn workspace chap-eval build",
    "build:prediction-app": "yarn workspace prediction-app build",
    "build": "yarn build:chap-lib && yarn build:chap-eval && yarn build:prediction-app",
    "test:chap-lib": "yarn workspace @dhis2-chap/chap-lib test",
    "test:chap-eval": "yarn workspace chap-eval test",
    "test:prediction-app": "yarn workspace prediction-app test",
    "test": "echo \"todo: implement tests\"",
    "i18n:chap-lib": "yarn workspace @dhis2-chap/chap-lib d2-app-scripts i18n generate",
    "i18n:prediction-app": "yarn workspace prediction-app d2-app-scripts i18n generate",
    "i18n": "yarn i18n:chap-lib && yarn i18n:prediction-app"
  },
  "dependencies": {
    "@dhis2/d2-i18n": "^1.1.3",
    "@dhis2/ui": "^9.11.7",
    "@types/highcharts": "^5.0.44",
    "highcharts": "11.4.8",
    "highcharts-react-official": "^3.2.1",
    "react-virtuoso": "^4.6.2",
    "typescript": "^5.7.2"
